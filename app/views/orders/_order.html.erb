<div id="<%= dom_id order %>">
  <p>
    <strong>Product name:</strong>
    <%= order.product_name %>
  </p>
  <p>
    <strong>Quantity:</strong>
    <%= order.product_count %>
  </p>

  <p>
    <strong>Customer ID:</strong>
    <%= order.customer_id %>
  </p>
  <div>
  <%= link_to "Edit order", edit_order_path(order) %> |
  <%= link_to "Destroy order", order, method: :delete %>
</div>

</div>
